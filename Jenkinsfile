pipeline{
    agent any
    stages{
        stage1('build'){
            steps{
                script{
                    echo "hello world"
                }
            }
        }
        stage2('production'){
            steps{
                script{
                    echo "hello big world"
                }
            }
        }
        stage3('test'){
            steps{
                script{
                    echo "test hello world"
                }
            }
        }
    }
}